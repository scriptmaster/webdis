{
  "redis_host": "127.0.0.1",
  "redis_port": 6379,
  "redis_auth": null,

	"http_host":	"127.0.0.1",
	"http_port":	7379,

	"run_in_background":	true,
  "threads": 4,

  "database": 0,

  "acl": [
    {
      "disabled": [ "DEBUG" ]
    },
    {
      "http_basic_auth": "user:password",
      "enabled": [ "DEBUG" ]
    }
  ],

  "hiredis": {
    "keep_alive_sec": 15
  },

  "verbosity": 3,
  "logfile": "/var/log/webdis.log"
}
